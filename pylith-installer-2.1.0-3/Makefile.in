all: installed_pylith 

rebuild-all: finished_mkdest compilers python mpi cppunit swig numpy proj4 netcdf hdf5 h5py fiat nemesis spatialdata petsc pylith

# ----------------------------------------------------------------------
# Note on configure invocations
#
# Normally, we would like to do configure ARGS && make && make
# install. However a bug in autoconf results in d.SYM directories
# generating an error when configure tries to remove them with "rm"
# rather than "rm -r". As a result, we use configure ARGS ; make &&
# make install.

# ----------------------------------------------------------------------
# setup
# ----------------------------------------------------------------------
mkdest:
	$(MKDIR_P) $(prefix)/include $(prefix)/lib $(prefix)/bin

finished_mkdest:
	$(MAKE) $(AM_MAKEFLAGS) mkdest && touch $@

# ----------------------------------------------------------------------
# compilers
# ----------------------------------------------------------------------
compilers: finished_mkdest
@INSTALL_GCC_TRUE@	$(FETCH_SCRIPT) "$(downloader)" gcc-$(GCC_VER).tar.bz2 $(DEPS_URL)
@INSTALL_GCC_TRUE@	$(FETCH_SCRIPT) "$(downloader)" mpc-$(MPC_VER).tar.gz $(DEPS_URL)
@INSTALL_GCC_TRUE@	$(FETCH_SCRIPT) "$(downloader)" gmp-$(GMP_VER).tar.gz $(DEPS_URL)
@INSTALL_GCC_TRUE@	$(FETCH_SCRIPT) "$(downloader)" mpfr-$(MPFR_VER).tar.bz2 $(DEPS_URL)
@INSTALL_GCC_TRUE@	$(TAR) -jxf gcc-$(GCC_VER).tar.bz2
@INSTALL_GCC_TRUE@	cd gcc-$(GCC_VER) ; \
@INSTALL_GCC_TRUE@		tar -zxf ../gmp-$(GMP_VER).tar.gz; \
@INSTALL_GCC_TRUE@		ln -sf gmp-$(GMP_VER)/ gmp; \
@INSTALL_GCC_TRUE@		tar -zxf ../mpc-$(MPC_VER).tar.gz; \
@INSTALL_GCC_TRUE@		ln -sf mpc-$(MPC_VER)/ mpc; \
@INSTALL_GCC_TRUE@		tar -jxf ../mpfr-$(MPFR_VER).tar.bz2; \
@INSTALL_GCC_TRUE@		ln -sf mpfr-$(MPFR_VER)/ mpfr
@INSTALL_GCC_TRUE@	$(MKDIR_P) gcc-build
@INSTALL_GCC_TRUE@	cd gcc-build && \
@INSTALL_GCC_TRUE@		../gcc-$(GCC_VER)/configure --prefix=$(prefix) \
@INSTALL_GCC_TRUE@			--disable-multilib \
@INSTALL_GCC_TRUE@			--enable-languages=$(gcc_languages) \
@INSTALL_GCC_TRUE@			$(env_flags) && \
@INSTALL_GCC_TRUE@		make -j $(make_threads) $(env_flags) && \
@INSTALL_GCC_TRUE@		make install && \
@INSTALL_GCC_TRUE@		touch ../installed_compilers

@GCC_FORTRAN_ONLY_TRUE@@INSTALL_GCC_TRUE@	mv $(prefix)/bin/gcc $(prefix)/bin/gcc-$(GCC_VER)

@INSTALL_GCC_FALSE@	@echo "$@ already installed. Skipping installation."
@INSTALL_GCC_FALSE@	touch installed_compilers

installed_compilers:
	$(MAKE) $(AM_MAKEFLAGS) compilers

